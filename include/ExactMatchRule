#ifndef EMIT_EXACTMATCHRULE_HEADER
#define EMIT_EXACTMATCHRULE_HEADER

#include <functional>
#include <vector>

class ExactMatchRule
{
    const std::string constant;
public:
    ExactMatchRule(const std::string& constant) :
        constant(constant)
    {
    }

    bool match(const std::string& candidate);
};

#endif // EMIT_EXACTMATCHRULE_HEADER

